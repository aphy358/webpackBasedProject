<!--当季热销模板-->
<% for(var i = 0; i < 5; i++) {%>
    <li class="ad-one-wrap <%= (i % 5 == 4 ? 'no-mr' : '') %>" data-src="<%= arr[i].adLink %>">
        <img src="<%= arr[i].adImg %>" class="too-width" alt="">
        <div class="ad-one-shadow"></div>
        <div class="ad-one-text"><%= arr[i].adTitle %></div>
    </li>
<% } %>