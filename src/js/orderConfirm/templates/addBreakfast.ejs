<span>加早信息</span>
<div class="other-msg">
  <ul>
    <% for(var i = 0;i < data[0].length;i++){ %>
    <li>
      <%= data[0][i].name %>
      ￥
      <%= data[0][i].price %>
      /份
    </li>
    <% } %>
    <li class="open-detail-msg">+</li>
  </ul>
</div>
<div class="open-add-msg">
  <div class="add-msg-head">
    <% for(var i = 0;i < data[0].length;i++){ %>
    <%= data[0][i].name %>
    ￥
    <%= data[0][i].price %>
    / 份&nbsp;&nbsp;&nbsp;
    <% } %>
  </div>
  <div class="add-msg-option">
    <ul>
      <li>
        <input class="breakfast-start start-date" type="text" value="<%= startDate %>">
      </li>
      <li>至</li>
      <li>
        <input class="breakfast-end end-date" type="text" value="<%= endDate %>">
      </li>
      <li>
        <select name="add-type" id="add-type">
          <% for(var i = 0;i < data[0].length;i++){ %>
          <option value="<%= data[0][i].name %>" add-breakfast="<%= data[0][i].price %>"><%= data[0][i].name %></option>
          <% } %>
        </select>
      </li>
      <li>
        <input class="breakfast-num" type="text" value="1">
      </li>
      <li class="add-breakfast add-item" add-category="add-breakfast">+</li>
    </ul>
  </div>
</div>

